<%- include("partials/header")  -%>
<h1>Compose</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="title">Title</label>
  <input id="title" type="text" name="composeTitle" class="form-control">
  </div>

  <div class="form-group">
    <label for="post">Post</label>
  <textarea id="post" type="text" name="composePost" class="form-control" rows="5"></textarea>
  </div>

<button type="submit" name="button" class="btn btn-primary">Publish</button>
</form>



<%- include("partials/footer")  -%>
